{
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)\\.js",
      "dest": "/$1.js"
    },
    {
      "src": "/(.*)\\.css",
      "dest": "/$1.css"
    },
    {
      "src": "/(.*)\\.json",
      "dest": "/$1.json"
    },
    {
      "src": "/(.*)\\.png",
      "dest": "/$1.png"
    },
    {
      "src": "/(.*)\\.jpg",
      "dest": "/$1.jpg"
    },
    {
      "src": "/(.*)\\.svg",
      "dest": "/$1.svg"
    },
    {
      "src": "/(.*)\\.ico",
      "dest": "/$1.ico"
    },
    {
      "src": "/(.*)\\.pdf",
      "dest": "/$1.pdf"
    },
    {
      "src": "/(.*)\\.pptx?",
      "dest": "/$1.pptx"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}